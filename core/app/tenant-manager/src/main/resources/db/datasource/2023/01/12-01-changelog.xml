<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

	<changeSet id="1673543649792-1" author="openk9">
		<createTable tableName="file_resource">
			<column name="id" type="BIGINT">
				<constraints nullable="false" primaryKey="true"
						 primaryKeyName="file_resource_pkey"/>
			</column>
			<column name="create_date" type="TIMESTAMP WITHOUT TIME ZONE"/>
			<column name="modified_date" type="TIMESTAMP WITHOUT TIME ZONE"/>
			<column name="fileId" type="VARCHAR(255)">
				<constraints nullable="false"/>
			</column>
			<column name="resourceId" type="VARCHAR(255)">
				<constraints nullable="false"/>
			</column>
			<column name="datasourceId" type="VARCHAR(255)">
				<constraints nullable="false"/>
			</column>
		</createTable>
	</changeSet>
	<changeSet id="1673543649792-2" author="openk9">
		<addUniqueConstraint
			columnNames="fileId, datasourceId"
			constraintName="uc_fileresource_fileid_datasourceId"
			tableName="file_resource"/>
	</changeSet>
	<changeSet id="1673543649792-3" author="openk9">
		<addUniqueConstraint
			columnNames="resourceId"
			constraintName="uc_fileresource_resourceid"
			tableName="file_resource"/>
	</changeSet>
	
</databaseChangeLog>