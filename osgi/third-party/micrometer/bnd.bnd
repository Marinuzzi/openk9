Bundle-Name: [OpenK9 - Third Party] Micrometer
Bundle-SymbolicName: io.openk9.third.party.micrometer

Include-Resource: \
 @micrometer-core-*.jar,\
 @LatencyUtils-*.jar,\
 @HdrHistogram-*.jar

Export-Package: \
 io.micrometer.*, \
 org.HdrHistogram, \
 org.LatencyUtils

Import-Package: \
 !io.micrometer.*,\
 !org.LatencyUtils,\
 !org.HdrHistogram.*,\
 !org.HdrHistogram,\
 !*;resolution:=optional


-fixupmessages: Classes found in the wrong directory: ...;is:=ignore

-include: ${project.rootDir}/osgi/common.bnd