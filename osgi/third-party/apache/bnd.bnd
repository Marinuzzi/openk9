Bundle-Name: [OpenK9 - Third Party] Apache Lib
Bundle-SymbolicName: io.openk9.third.party.apache.lib

Include-Resource: @commons-lang3-*.jar

Export-Package: \
 org.apache.commons.lang3, \
 org.apache.commons.lang3.concurrent.locks, \
 org.apache.commons.lang3.text.translate, \
 org.apache.commons.lang3.*,\
 org.apache.commons.codec.*

Import-Package: !org.apache.commons.lang3

-fixupmessages: Classes found in the wrong directory: ...;is:=ignore

-include: ${project.rootDir}/osgi/common.bnd